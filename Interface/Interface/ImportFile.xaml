<Window x:Class="Interface.ImportFile"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Interface"
        mc:Ignorable="d"
             WindowState="Maximized"
        Title="ImportFile" Height="720" Width="1280">

    <Grid>
        <Label Content="HRAS Import Info" FontSize="30" FontWeight="Bold" HorizontalAlignment="Center" VerticalAlignment="Top" Margin="0,20,0,0"/>
        <Button Content="Search" FontSize="13" HorizontalAlignment="Center" VerticalAlignment="Bottom" Margin="1052,0,60,609" Height="30" Width="160" Click="Button_Click_Back"/>
        <Button Content="Main Menu" Background="White" FontWeight="Bold" HorizontalAlignment="Left" VerticalAlignment="Bottom"  Margin="60,0,0,30" Height="35" Width="75" Click="Button_Click_BackMenu" />
        <Button Content="Log out" Background="White" FontWeight="Bold" HorizontalAlignment="Right" VerticalAlignment="Bottom"  Margin="0,0,60,30" Height="35" Width="75" Click="Button_Click_LogOut" />
        <DataGrid HorizontalAlignment="Left" Height="487" Margin="26,109,0,0" VerticalAlignment="Top" Width="1222" Name="I1" ItemsSource="{Binding}" AutoGenerateColumns="False" SelectionChanged="IR_DataChange"/>
    </Grid>
</Window>
